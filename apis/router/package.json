{
  "name": "@sushiswap/router-api",
  "version": "0.0.0",
  "private": true,
  "description": "Sushi Router API",
  "keywords": [
    "sushi",
    "router",
    "api"
  ],
  "homepage": "https://www.sushi.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "apis/router"
  },
  "license": "MIT",
  "author": "Ilya Lyalin <bretnwood@gmail.com>",
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "compair-test": "ts-node --skip-project ./test/compair.ts",
    "dev": "tsc -w",
    "load-test": "ts-node --skip-project ./test/load.ts",
    "artillery": "artillery run test/load.yml",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@sentry/node": "7.77.0",
    "@sushiswap/extractor": "workspace:*",
    "@sushiswap/router": "workspace:*",
    "@sushiswap/router-config": "workspace:*",
    "@sushiswap/tines": "workspace:*",
    "@sushiswap/v2-sdk": "workspace:*",
    "@sushiswap/v3-sdk": "workspace:*",
    "@sushiswap/viem-config": "workspace:*",
    "@wagmi/core": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "fastify": "4.26.0",
    "sushi": "workspace:*",
    "overload-protection": "1.2.3",
    "pidusage": "3.0.2",
    "event-loop-lag": "1.4.0",
    "viem": "1.21.4",
    "wagmi": "1.4.5",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/node18": "18.2.2",
    "@tsconfig/strictest": "2.0.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/node": ">=20.8.4",
    "@types/pidusage": "2.0.5",
    "@types/overload-protection": "1.2.4",
    "@types/event-loop-lag": "1.0.33",
    "artillery": "2.0.5",
    "autocannon": "7.14.0",
    "ts-node": "10.9.2",
    "undici": "6.6.2",
    "typescript": "5.2.2"
  }
