{
  "name": "@sushiswap/steer-sdk",
  "version": "0.0.0",
  "description": "Sushi Steer SDK",
  "keywords": [
    "sushi",
    "steer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/steer-sdk"
  },
  "license": "MIT",
  "author": "LufyCZ <me@lufy.cz>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./abi": {
      "types": "./dist/abi/index.d.ts",
      "import": "./dist/abi/index.js",
      "default": "./dist/abi/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "source": "src/index.ts",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "src/index"
      ],
      "abi": [
        "src/abi/index"
      ]
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc --watch",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@steerprotocol/contracts": "2.1.0-beta.14",
    "@sushiswap/database": "workspace:*",
    "sushi": "workspace:*",
    "tslib": "latest",
    "viem": "2.10.11"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "20",
    "@wagmi/core": "2.10.6",
    "eslint": "8.43.0",
    "next": "15.0.0-canary.45",
    "react": "19.0.0-rc-8971381549-20240625",
    "react-dom": "19.0.0-rc-8971381549-20240625",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "@wagmi/core": "2.10.6",
    "react": "19.0.0-rc-8971381549-20240625",
    "react-dom": "19.0.0-rc-8971381549-20240625",
    "wagmi": "2.9.12"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "next": "15.0.0-canary.45"
  }
}
