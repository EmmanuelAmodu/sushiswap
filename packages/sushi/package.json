{
  "name": "sushi",
  "version": "3.0.0",
  "description": "Sushi",
  "keywords": [
    "sushi"
  ],
  "homepage": "https://www.sushi.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/sushi"
  },
  "license": "MIT",
  "author": "Matthew Lilley <hello@matthewLilley.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./abi": {
      "types": "./dist/abi/index.d.ts",
      "default": "./dist/abi/index.js"
    },
    "./chain": {
      "types": "./dist/chain/index.d.ts",
      "default": "./dist/chain/index.js"
    },
    "./currency": {
      "types": "./dist/currency/index.d.ts",
      "default": "./dist/currency/index.js"
    },
    "./format": {
      "types": "./dist/format/index.d.ts",
      "default": "./dist/format/index.js"
    },
    "./math": {
      "types": "./dist/math/index.d.ts",
      "default": "./dist/math/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "abi": [
        "./dist/abi/index.d.ts"
      ],
      "chain": [
        "./dist/chain/index.d.ts"
      ],
      "currency": [
        "./dist/currency/index.d.ts"
      ],
      "math": [
        "./dist/math/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "prepublishOnly": "pnpm build",
    "test": "vitest -c ./test/vitest.config.ts dev"
  },
  "dependencies": {
    "big.js": "6.1.1",
    "decimal.js-light": "2.5.1",
    "numeral": "2.0.6",
    "tiny-invariant": "1.3.1",
    "toformat": "2.0.0"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/node18": "18.2.2",
    "@tsconfig/strictest": "2.0.2",
    "@types/big.js": "6.2.0",
    "@types/jest": "29.5.5",
    "@types/numeral": "2.0.3",
    "jest": "29.7.0",
    "typescript": "5.2.2",
    "viem": "1.16.6",
    "vitest": "0.34.6",
    "zod": "3.21.4"
  },
  "peerDependencies": {
    "viem": "*",
    "zod": "*"
  },
  "peerDependenciesMeta": {
    "viem": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  }
}
