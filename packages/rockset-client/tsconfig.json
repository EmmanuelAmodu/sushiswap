  {
    "extends": "@sushiswap/typescript-config/esm.json",
    "exclude": ["node_modules", "dist"],
    "include": ["src"],
    "compilerOptions": {
      "outDir": "./dist",
      "rootDir": "./src",
      "target": "ESNext",
      "lib": ["ESNext"],
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "skipLibCheck": true,
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
    }
  }