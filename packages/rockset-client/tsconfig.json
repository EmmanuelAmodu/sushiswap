  {
    "extends": "@sushiswap/typescript-config/esm.json",
    "exclude": ["node_modules", "dist"],
    "include": ["src"],
    "compilerOptions": {
      "outDir": "./dist",
      "rootDir": "./src",
      "module": "esnext",
      "target": "esnext",
      "skipLibCheck": true,
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
      "moduleResolution": "nodenext"
    }
  }